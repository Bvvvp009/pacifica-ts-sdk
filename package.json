{
  "name": "pacifica-ts-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Pacifica  - Community built SDK for trading and account management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
